<div class="page-header">
  <%= link_to contacts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_contact_path(@contact), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contact</h1>
</div>

<dl class="dl-horizontal">
  <dt>Uuid:</dt>
  <dd><%= @contact.uuid %></dd>

  <dt>First name:</dt>
  <dd><%= @contact.first_name %></dd>

  <dt>Last name:</dt>
  <dd><%= @contact.last_name %></dd>

  <dt>Email:</dt>
  <dd><%= @contact.email %></dd>

  <dt>Phone mobile:</dt>
  <dd><%= @contact.phone_mobile %></dd>

  <dt>Street address:</dt>
  <dd><%= @contact.street_address %></dd>

  <dt>City:</dt>
  <dd><%= @contact.city %></dd>

  <dt>State:</dt>
  <dd><%= @contact.state %></dd>

  <dt>Zip code:</dt>
  <dd><%= @contact.zip_code %></dd>

  <dt>Is starred:</dt>
  <dd><%= @contact.is_starred %></dd>

  <dt>Birth month:</dt>
  <dd><%= @contact.birth_month %></dd>

  <dt>Birth day:</dt>
  <dd><%= @contact.birth_day %></dd>

</dl>
